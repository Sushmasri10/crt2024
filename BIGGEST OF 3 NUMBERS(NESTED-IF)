BIGGEST OF 3 NUMBERS(NESTED-IF)

#include <stdio.h>
int main() {
   int a,b,c;
   scanf("%d%d%d",&a,&b,&c);
   if(a>b){
       if(a>c){
           printf("A is big:%d",a);
              }
       else{
       printf("C is big:%d",c);
             }
       }
   else if(b>c)
   {
       printf("B is big:%d",b);
           }
           else
           {
               printf("C is big:%d",c);
           }
   return 0;
}
