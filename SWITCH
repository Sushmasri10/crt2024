SWITCH

#include <stdio.h>
int main() {
   int a;
   scanf("%d",&a);
   switch(a){
       case 1:
       printf("Java");
       break;
      case 2:
      printf("Python");
      break;
      case 3:
      printf("C");
      break;
      case 4:
      printf("C++");
      break;
      default:
      printf("Invalid");
   }
   return 0;
}
