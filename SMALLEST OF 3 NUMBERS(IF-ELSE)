SMALLEST OF 3 NUMBERS(IF-ELSE)

#include <stdio.h>
int main() {
    int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    if((a<b)&&(a<c)){
        printf("A is small:%d",a);
        }
        else
        if(b<c){
            printf("B is smallest:%d",b);
        }
        else
        {
            printf("C is smallest:%d",c);
        }
    return 0;
}
