ARITHMETIC OPERATORS USING SWITCH

#include <stdio.h>
int main() {
   int a,b;
   scanf("%d%d",&a,&b);
   int ch;
   scanf("%d",&ch);
   switch(ch){
       case 1:
       printf("a+b is:%d",a+b);
       break;
       case 2:
       printf("a-b is:%d",a-b);
       break;
       case 3:
       printf("a*b is:%d",a*b);
       break;
       case 4:
       printf("a/b is:%d",a/b);
       break;
       default:
       printf("Invalid choice");
   }
}
