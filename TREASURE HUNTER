TREASURE HUNTER HACKERRANK

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int a,b,c;
    scanf("%d%d%d",&a,&b,&c);
    int r=a*(b)/100;
    printf("%d",r);
    int r1=(a-r)*(c)/100;
    printf("\n%d",r1);
    int r2=(a-r-r1)/3;
    printf("\n%d",r2);
    return 0;

}
