ELECTRICITY BILL HACKERRANK


#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int n;
    scanf("%d",&n);
    if(n<=200){
        int cost=(n*0.5);
        printf("Rs.%d",cost);
    }
    else if(n<=400){
        int cost1=(n*0.65)+100;
        printf("Rs.%d",cost1);
    }
    else if(n<=600){
        int cost2=(n*0.8)+200;
        printf("Rs.%d",cost2);
    }
    else if(n>600){
        int cost3=(n*1.25)+425;
        printf("Rs.%d",cost3);
    }
    
    
    return 0;
}
